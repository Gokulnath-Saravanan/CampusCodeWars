{
  "name": "campus-code-wars",
  "version": "1.0.0",
  "description": "A competitive coding platform for campus events",
  "main": "index.js",
  "scripts": {
    "start": "node server/src/index.js",
    "server": "nodemon server/src/index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install"
  },
  "keywords": ["coding", "competition", "campus", "mern"],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3"
  }
} 